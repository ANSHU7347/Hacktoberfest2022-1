import java.util.*;
import java.lang.*;
import java.io.*;

/* Name of the class has to be "Main" only if the class is public. */
class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner sc=new Scanner(System.in);
		int t=sc.nextInt();
		int k=1;
		while(k<=t){
		    int x=sc.nextInt();
		    int d=3000;
		    int e=x*10;
		    if(x<=300){
		        System.out.println(d);
		    }
		    else{
		        System.out.println(e);
		    }
		    k++;
		}
	}
}
